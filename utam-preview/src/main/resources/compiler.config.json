{
  "pageObjectsRootDir": "/src/main/resources/spec",
  "pageObjectsFilesMask": "(.*)\\.utam\\.json$",
  "pageObjectsOutputDir": "/src/main/java",
  "resourcesOutputDir": "/src/main/resources",
  "unitTestsOutputDir": "/src/test/java",
  "unitTestsRunner": "NONE",
  "profiles": [
    {
      "name": "platform",
      "values": [
        "ios_phone",
        "ios_tablet",
        "android_phone",
        "android_tablet"
      ]
    }
  ],
  "namespaces": [
    {
      "typeMatch": "utam-lightning",
      "pathMatch": ".*/lightning"
    },
    {
      "typeMatch": "utam-force",
      "pathMatch": ".*/force"
    },
    {
      "typeMatch": "utam-tests",
      "pathMatch": ".*/tests"
    },
    {
      "typeMatch": "utam-global",
      "pathMatch": ".*/one"
    },
    {
      "typeMatch": "utam-record-flexipage",
      "pathMatch": ".*/record_flexipage"
    },
    {
      "typeMatch": "utam-flexipage",
      "pathMatch": ".*/flexipage"
    },
    {
      "typeMatch": "utam-records",
      "pathMatch": ".*/records"
    },
    {
      "typeMatch": "utam-action",
      "pathMatch": ".*/runtime_platform_actions"
    },
    {
      "typeMatch": "utam-salesforceapp",
      "pathMatch": ".*/mobile"
    },
    {
      "typeMatch": "utam-sfdx",
      "pathMatch": ".*/force-app"
    },
    {
      "typeMatch": "utam-sfa",
      "pathMatch": ".*/sfa"
    },
    {
      "typeMatch": "utam-portal",
      "pathMatch": ".*/portal"
    }
  ]
}
